<% layout("/layouts/boilerplate") %>
<body>
    <h1>New Listing</h1>
    <form method="POST" action="/listings">
        <input placeholder="Title" name="listing[title]"><br><br>
        <input placeholder="Description" name="listing[description]"><br><br>
        <input placeholder="Price" name="listing[price]"><br><br>
        <input placeholder="location" name="listing[location]"><br><br>
        <input placeholder="Country" name="listing[country]"><br><br>
        <button>Submit</button>
    </form>
</body>
